 arch/arm/mach-imx/eukrea_mbimxsd25-baseboard.c |   24 ++++++++++++++++++++----
 1 files changed, 20 insertions(+), 4 deletions(-)

diff --git a/arch/arm/mach-imx/eukrea_mbimxsd25-baseboard.c b/arch/arm/mach-imx/eukrea_mbimxsd25-baseboard.c
index 384051d..377ceac 100644
--- a/arch/arm/mach-imx/eukrea_mbimxsd25-baseboard.c
+++ b/arch/arm/mach-imx/eukrea_mbimxsd25-baseboard.c
@@ -152,8 +152,8 @@ static struct imx_fb_videomode eukrea_mximxsd_modes[] = {
 			.upper_margin	= 7,
 			.lower_margin	= 100,
 		},
-		.pcr		= 0xFA208B80,
-		.bpp		= 16,
+		.pcr	= 0xFA208B80,
+		.bpp	= 16,
 	}, {
 		.mode = {
 			.name		= "DVI-SVGA",
@@ -168,8 +168,24 @@ static struct imx_fb_videomode eukrea_mximxsd_modes[] = {
 			.upper_margin	= 7,
 			.lower_margin	= 75,
 		},
-		.pcr		= 0xFA208B80,
-		.bpp		= 16,
+		.pcr	= 0xFA208B80,
+		.bpp	= 16,
+	}, {
+		.mode	= {
+			.name		= "43-WQVGA",
+			.refresh	= 60,
+			.xres		= 480,
+			.yres		= 272,
+			.pixclock	= KHZ2PICOS(9072),
+			.hsync_len	= 10,
+			.left_margin	= 40,
+			.right_margin	= 5,
+			.vsync_len	= 10,
+			.upper_margin	= 8,
+			.lower_margin	= 8,
+		},
+		.pcr	= 0xCAE08B80,
+		.bpp	= 16,
 	},
 };
 
